(defn tail [_ ...rest] rest)

(defn nil? [x] (= '() x))

(defn -pairs [a b ...rest]
    (cons
        (list a b)
        (if (nil? rest) 
            '()
            (ap -pairs rest))))

(defn pairs [ls] (ap -pairs ls))

(defn range [s e]
    (if (= s e)
        (list)
        (cons s (range (+ s 1) e))))

(defn -filter [f x ...ls]
    (if (f x)
        (if (nil? ls)
            (list x)
            (cons x (filter f ls)))
        (if (nil? ls)
            (list)
            (cons x (filter f ls)))))
(defn filter [f ls]
    (-filter f ls))
