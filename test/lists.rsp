(defn tail [_ ...rest] rest)

(defn nil? [x] (= '() x))


(defn -pairs [a b ...rest]
    (cons
        (list a b)
        (if (nil? rest) 
            '()
            (ap -pairs rest))))

(defn pairs [ls] (ap -pairs ls))

(defn range [s e]
    (if (= s e)
        '()
        (cons s (range (+ s 1) e))))
